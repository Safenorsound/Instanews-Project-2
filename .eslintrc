{
  "extends": "eslint:recommended",
  "rules": {
    
    "no-alert": 0,
    "camelcase": 0,
    "curly": 0,
    "eqeqeq": 0,
    "no-console": 0,
    "guard-for-in": 0,
    "no-empty": 0,
    "no-param-reassign": 0,
    "no-unused-vars": 0,
    "quotes": [ 0, "single"]
  },
  "globals": {
    "jQuery": false,
    "$": false  },
    "env": {

    "browser": true
  }
}